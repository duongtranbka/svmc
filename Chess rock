#include <iostream>
using namespace std;

int result = 0 ;
int n;
int total;
char arr[4][4];


int dx[4] = {0,0,1,-1};
int dy[4] = {1,-1,0,0}; 

void xuat()
{
	for (int i = 0; i < n ; i++)
	{
		for (int j = 0; j < n  ; j++)
		{
			cout<<arr[i][j];
		}
		cout<<endl;
	}
	cout<<endl;
}


void check()
{
	int test = 0;

	for (int i = 0; (i < n) && (test == 0) ; i++)
	{
		for (int j = 0; (j < n) && (test == 0) ; j++)
		{
			if(arr[i][j] == '1')
			{
				for (int k = 0; k < 4; k++)
				{
					int x = i + dx[k];
					int y = j + dy[k];
					while ( (x>= 0 && x<n) && (y>=0 && y<n) )
					{
						if (arr[x][y] == 'X' )
						{
						  	break;
						}
						if (arr[x][y] == '1' )
						{
							test = 1;							
							break;
						
						}
						x+= dx[k];
						y+= dy[k];
						
					}
				}
			}
		}
	}


	if(test == 0 )
	{
		int count = 0;
		for (int i = 0; i < n ; i++)
		{
			for (int j = 0; j < n  ; j++)
			{
				if (arr[i][j] == '1')
				{
					count++;
				}
			}
		}
		if(count >= result)
		{
			result = count;
		}
	}


}

void play(int k)
{

	for (int i = 0; i <= 1; i++)
	{
		char tmp = '0' + i;

		if( arr[k/n][k%n] != 'X')
		{
			arr[k/n][k%n] = tmp;	

			if( k == total )
			{
				//xuat();
				check();
				break;
			}
			else
			{
				play(k+1);
			}
		}
		else
		{
				play(k+1);
				break;
		}
		
	}

}


int main()
{
	//freopen("Text.txt","r", stdin);
	int T;
	cin >> T;


	for(int tc = 1; tc <= T; tc++)
	{
		//int n;
		cin>>n;

		for (int i = 0; i < 4; i++)
		{
			for (int j = 0; j < 4; j++)
			{
				arr[i][j] = 0;
			}
		}

		for (int i = 0; i < n; i++)
		{
			for (int j = 0; j < n; j++)
			{
				cin>>arr[i][j];
			}
		}

		::result = 0;

		total = n*n;
		//	xuat();
		play(0);

		cout<<"Case #"<<tc<<endl<<result<<endl;

	}

	return 0;
}
