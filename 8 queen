#include<iostream>
using namespace std;
int n=8;
int map[20][20];
int ans=0;
bool h[11],c[11],nguoc[22],xuoi[22];
int a[11];
void test(int x)
{
	for(int i=0;i<n;i++)
	{
		if(!h[x] && !c[i] && !xuoi[x+i-1] && !nguoc[x-i+n] )
		{
			a[x]=i;
			h[x]=c[i]=xuoi[x+i-1]=nguoc[x-i+n]=true;
			if(x==n-1)
			{
				int dem=0;
				for(int k=0;k<n;k++)
				{
					dem=dem+map[k][a[k]];
				}
				if(dem>ans) ans=dem;
			}
			else test(x+1);
			h[x]=c[i]=xuoi[x+i-1]=nguoc[x-i+n]=false;
		}
	}
}
int main()
{
	//freopen("input.txt","r",stdin);
	int t;
	cin >> t;
	for(int tc=1;tc<=t;tc++)
	{
		int p;
		cin >> p;
		cout << "Case" << " "<< "#" << tc <<  endl;
		for(int k=0;k<p;k++)
		{
            ans=0;
			for(int i=0;i<n;i++)
			{
				for(int j=0;j<n;j++)
				{
					cin >> map[i][j];
				}
			}
			test(0);
			cout << ans << endl;
		}
	}
	return 0;
}
